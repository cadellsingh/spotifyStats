]);\n\n  const { description, imageUrl, items, name } = playlistInfo;\n\n  const displayTracks =\n    items &&\n    items.map((data, index) => {\n      const { track } = data;\n      return <DisplayTrack key={index} data={track} />;\n    });\n\n  const displayImg = imageUrl ? (\n    <img src={imageUrl} alt={name} />\n  ) : (\n    <span>\n      <FontAwesomeIcon icon={faHeadphonesAlt} />\n    </span>\n  );\n\n  return (\n    <Container\n      data-aos=\"fade-down\"\n      data-aos-duration=\"1500\"\n      data-aos-easing=\"ease-in-out\"\n    >\n      <PlaylistInfo>\n        {displayImg}\n        <div>\n          <h3>{name}</h3>\n          <p>{description}</p>\n        </div>\n      </PlaylistInfo>\n      <PlaylistTracks>{displayTracks}</PlaylistTracks>\n    </Container>\n  );\n};\n\nexport default Playlist;\n",["182","183"],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Artists/Artist.js",["184"],"import { useParams } from \"react-router-dom\";\nimport { useState, useEffect } from \"react\";\nimport styled from \"styled-components\";\nimport { getArtist } from \"../../spotify/apis\";\nimport { ContainerBackground } from \"../../styles/sharedStyles\";\nimport { formatNumber } from \"../../utils/functions\";\nimport ArtistTopTracks from \"./ArtistTopTracks\";\nimport RelatedArtists from \"./RelatedArtists\";\n\nconst Container = styled.div`\n  display: grid;\n  grid-template-columns: 1.8fr 1fr;\n  grid-column-gap: 15px;\n\n  & h3 {\n    ${ContainerBackground};\n    font-size: 22px;\n    margin: 15px 0;\n  }\n\n  @media (max-width: 960px) {\n    grid-template-columns: 1fr;\n  }\n`;\n\nconst ArtistDetails = styled.div`\n  grid-column: span 2 / auto;\n  ${ContainerBackground};\n\n  & div:first-child {\n    display: flex;\n  }\n\n  & div:first-child div {\n    display: flex;\n    flex-direction: column;\n    margin-left: 25px;\n    margin-top: 25px;\n  }\n\n  & h2 {\n    font-size: 25px;\n    text-align: left;\n  }\n\n  & p {\n    font-size: 20px;\n  }\n\n  & p:first-of-type {\n    margin: 6px 0;\n  }\n  & img {\n    max-width: 100%;\n    width: 200px;\n    height: auto;\n    border-radius: 10px;\n    height: auto;\n  }\n`;\n\nconst Artist = () => {\n  const { artistId } = useParams();\n  const [artistInfo, setArtistInfo] = useState([]);\n\n  const getData = async () => {\n    const { total, genres, imageUrl, name } = await getArtist(artistId);\n    setArtistInfo({\n      total,\n      genres,\n      imageUrl,\n      name,\n    });\n  };\n\n  useEffect(() => {\n    getData();\n  }, []);\n\n  const { total, genres, imageUrl, name } = artistInfo;\n\n  return (\n    <div\n      data-aos=\"fade-down\"\n      data-aos-duration=\"1500\"\n      data-aos-easing=\"ease-in-out\"\n    >\n      <ArtistDetails>\n        <div>\n          <img src={imageUrl} alt={name} />\n          <div>\n            {/* data is undefined */}\n            <h2>{name}</h2>\n            <p>Followers: {formatNumber(total)}</p>\n            <p>{genres && genres.join(\", \")}</p>\n          </div>\n        </div>\n      </ArtistDetails>\n      <Container>\n        <ArtistTopTracks artistId={artistId} />\n        <RelatedArtists artistId={artistId} />\n      </Container>\n    </div>\n  );\n};\n\nexport default Artist;\n","/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/RecentlyPlayed/RecentlyPlayedTracks.js",[],["185","186"],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Playlists/GlobalTopPlaylists.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Header.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/index.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/spotify/loginUrl.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Artists/TopArtists.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Playlists/UserPlaylists.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/NavListItem.js",[],"/Users/cadellsingh/Javascrip