   <ArtistDetails>\n        <div>\n          <img src={imageUrl} alt={name} />\n          <div>\n            {/* data is undefined */}\n            <h2>{name}</h2>\n            <p>Followers: {formatNumber(total)}</p>\n            <p>{genres && genres.join(\", \")}</p>\n          </div>\n        </div>\n      </ArtistDetails>\n      <Container>\n        <ArtistTopTracks artistId={artistId} />\n        <RelatedArtists artistId={artistId} />\n      </Container>\n    </div>\n  );\n};\n\nexport default Artist;\n",["175","176"],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/RecentlyPlayed/RecentlyPlayedTracks.js",[],["177","178"],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Playlists/GlobalTopPlaylists.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Header.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/index.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/spotify/loginUrl.js",[],["179","180"],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Artists/TopArtists.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Playlists/UserPlaylists.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/NavListItem.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/styles/sharedContainers.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/DisplayImg.js",["181"],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Playlists/Playlists.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Profile.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Tracks/TopTracks.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/spotify/apis.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Artists/ArtistTopTracks.js",["182"],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Artists/RelatedArtists.js",["183"],"import styled from \"styled-components\";\nimport { useState, useEffect } from \"react\";\nimport { ContainerBackground } from \"../../styles/sharedStyles\";\nimport { getArtistRelatedArtists } from \"../../spotify/apis\";\nimport DisplayImg from \"../DisplayImg\";\n\nconst Container = styled.div`\n  grid-row: span 2 / auto;\n`;\n\nconst Artists = styled.div`\n  grid-row: span 2 / auto;\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  grid-gap: 10px;\n  ${ContainerBackground};\n`;\n\nconst RelatedArtists = ({ artistId }) => {\n  const [relatedArtists, setRelatedArtists] = useState([]);\n\n  const getData = async () => {\n    const data = await getArtistRelatedArtists(artistId);\n    setRelatedArtists(data);\n  };\n\n  useEffect(() => {\n    getData();\n  }, []);\n\n  const displayRelatedArtists =\n    relatedArtists &&\n    relatedArtists.slice(0, 10).map((data, index) => {\n      return <DisplayImg key={index} data={data} type=\"artist\" />;\n    });\n\n  return (\n    <Container>\n      <h3>Related Artists</h3>\n      <Artists>{displayRelatedArtists}</Artists>\n    </Container>\n  );\n};\n\nexport default RelatedArtists;\n","/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/RecentlyPlayed/AllRecentlyPlayed.js",[],["184","185"],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/SpotifyPlaylists/EachCategory.js",["186"],"import styled from \"styled-components\";\nimport { useState, useEffect } from \"react\";\nimport { ContainerBackground } from \"../../styles/sharedStyles\";\nimport { getCategoryPlaylists } from \"../../spotify/apis\";\nimport { Link } from \"react-router-dom\";\n\nconst Container = styled.div`\n  ${ContainerBackground};\n\n  & h3 {\n    font-size: 22px;\n  }\n`;\n\nconst Playlists = styled.div`\n  margin-top: 10px;\n  display: grid;\n  grid-template-columns: repeat(5, 1fr);\n  grid-gap: 15px;\n\n  & span:hover {\n    text-decoration: underline;\n  }\n\n  @media (min-width: 1500px) {\n    span {\n      font-size: 18px;\n    }\n  }\n\n  @media 