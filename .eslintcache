an 2 / auto;\n`;\n\nconst Artists = styled.div`\n  grid-row: span 2 / auto;\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  grid-gap: 10px;\n  ${ContainerBackground};\n`;\n\nconst RelatedArtists = ({ artistId, setReloadArtist }) => {\n  const [relatedArtists, setRelatedArtists] = useState([]);\n\n  const getData = async () => {\n    const data = await getArtistRelatedArtists(artistId);\n    setRelatedArtists(data);\n  };\n\n  useEffect(() => {\n    getData();\n  }, []);\n\n  const displayRelatedArtists =\n    relatedArtists &&\n    relatedArtists.slice(0, 10).map((data, index) => {\n      return (\n        <DisplayImg\n          key={index}\n          data={data}\n          type=\"artist\"\n          setReloadArtist={setReloadArtist}\n        />\n      );\n    });\n\n  return (\n    <Container>\n      <h3>Related Artists</h3>\n      {relatedArtists.length > 0 ? (\n        <Artists>{displayRelatedArtists}</Artists>\n      ) : null}\n    </Container>\n  );\n};\n\nexport default RelatedArtists;\n",["239","240"],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/RecentlyPlayed/AllRecentlyPlayed.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/SpotifyPlaylists/EachCategory.js",["241"],"import styled from \"styled-components\";\nimport { useState, useEffect } from \"react\";\nimport { ContainerBackground } from \"../../styles/sharedStyles\";\nimport { getCategoryPlaylists } from \"../../spotify/apis\";\nimport { Link } from \"react-router-dom\";\n\nconst Container = styled.div`\n  ${ContainerBackground};\n\n  & h3 {\n    font-size: 22px;\n  }\n`;\n\nconst Playlists = styled.div`\n  margin-top: 10px;\n  display: grid;\n  grid-template-columns: repeat(5, 1fr);\n  grid-gap: 15px;\n\n  & span:hover {\n    text-decoration: underline;\n  }\n\n  @media (min-width: 1500px) {\n    span {\n      font-size: 18px;\n    }\n  }\n\n  @media (max-width: 1100px) {\n    grid-template-columns: repeat(4, 1fr);\n  }\n\n  @media (max-width: 900px) {\n    grid-template-columns: repeat(3, 1fr);\n  }\n\n  @media (max-width: 500px) {\n    grid-template-columns: repeat(2, 1fr);\n  }\n`;\n\nconst category = (arr) => {\n  const temp =\n    arr &&\n    arr.map((playlist, index) => {\n      const { name: playlistName, id } = playlist;\n\n      return (\n        <Link key={index} to={`/playlist/${id}`}>\n          <span>{playlistName}</span>\n        </Link>\n      );\n    });\n\n  return temp;\n};\n\nconst EachCategory = ({ categoryId, name, query }) => {\n  const [playlists, setPlaylists] = useState([]);\n\n  const getData = async () => {\n    const data = await getCategoryPlaylists(categoryId);\n    setPlaylists(data);\n  };\n\n  useEffect(() => {\n    getData();\n  }, []);\n\n  const filter = playlists.filter((playlist) => {\n    const { name: playlistName } = playlist;\n    return playlistName.toLowerCase().includes(query.toLowerCase());\n  });\n\n  const displayCategory = query === \"\" ? category(playlists) : category(filter);\n\n  return (\n    <>\n      {displayCategory.length !== 0 && (\n        <Container>\n          <h3>{name}</h3>\n          <Playlists>{displayCategory}</Playlists>\n        </Container>\n      )}\n    </>\n  );\n};\n\nexport default EachCategory;\n","/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Tracks/UserTopTracks.js",[],["242","243"],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Artists/UserTopArtists.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/SpotifyPlaylists/Categories.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Nav/Nav.js",[],["244","245"],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/DisplayTrack.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/NotFound.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/components/Nav/MobileNav.js",[],"/Users/cadellsingh/Javascript/apps/spotify-stats/src/spotify/tokens.js",[],"/Users/cadellsingh/Javascript/